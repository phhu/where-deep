{
  "name": "where-deep",
  "version": "0.0.6",
  "description": "A deep (recursive) version of Ramda.js's where and whereEq, allowing nested JSON-like structures to be used as predicates (for filtering and so on) for use on similar JSON-like structures",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "testmon": "nodemon --watch . --exec 'mocha ./test || true'",
    "tidy": "standard --fix",
    "publish": "np"
  },
  "files": [
    "arrayWhere.mjs",
    "permutate.mjs",
    "index.mjs",
    "package.json"
  ],
  "keywords": [
    "JSON",
    "ramda",
    "where",
    "deep",
    "filter",
    "FP",
    "Functional programming",
    "predicate"
  ],
  "author": "phhu.git@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phhu/where-deep.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phhu/where-deep/issues"
  },
  "homepage": "https://github.com/phhu/where-deep#readme",
  "dependencies": {
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "standard": "^17.0.0"
  }
}
